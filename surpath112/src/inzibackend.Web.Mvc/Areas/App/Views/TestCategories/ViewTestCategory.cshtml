@using inzibackend.Web.Areas.App.Startup
@using inzibackend.Web.Areas.App.Models.TestCategories
@model TestCategoryViewModel
@using inzibackend.Web.TagHelpers
@{
ViewBag.CurrentPageName = AppPageNames.Host.TestCategories;
}
@section Scripts
{
<script abp-src="/view-resources/Areas/App/Views/TestCategories/CreateOrEdit.js" asp-append-version="true"></script>
}

<div class="content d-flex flex-column flex-column-fluid">

    @{
    var breadcrumbs = new List<BreadcrumbItem>()
        {
        new BreadcrumbItem("/App/TestCategories", L("TestCategories")),
        new BreadcrumbItem(L("Details")),
        };
        }
        <abp-page-subheader title='@L("Details")' breadcrumbs="breadcrumbs">
        </abp-page-subheader>

        <div class="@(await GetContainerClass())">
            <div class=" card card-custom gutter-b">
                <div class=" card-body">
                    <div id="TestCategoryInformationsTab">
                        <div class="row">
                            <div class="col-6 mb-3">
                                <h6>@L("Name") :</h6>
                                @Model.TestCategory.Name
                            </div>
                            <div class="col-6 mb-3">
                                <h6>@L("InternalName") :</h6>
                                @Model.TestCategory.InternalName
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
</div>